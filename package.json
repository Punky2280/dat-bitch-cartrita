{
  "name": "dat-bitch-cartrita-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/backend",
    "packages/frontend",
    "packages/shared",
    "packages/robotics",
    "packages/mcp-core",
    "packages/orchestrator",
    "packages/supervisor-intelligence",
    "packages/mcp-testing",
    "packages/cartrita-v2",
    "apps/backend-v2",
    "apps/frontend-v2",
    "apps/worker",
    "apps/rag-service",
    "apps/fastapi-service",
    "packages/prompts",
    "packages/ui-components",
    "packages/config",
    "packages/rag-evaluation"
  ],
  "scripts": {
    "dev": "npm run dev:v2-backend",
    "dev:docker": "docker-compose -f docker-compose.dev-unified.yml up --build",
    "dev:logs": "docker-compose -f docker-compose.dev-unified.yml logs -f",
    "dev:down": "docker-compose -f docker-compose.dev-unified.yml down",
    "dev:clean": "docker-compose -f docker-compose.dev-unified.yml down -v",
    "dev:token": "node scripts/dev/get-auth-token.js",
    "dev:validate-users": "node scripts/dev/validate-users.js",
    "dev:frontend": "npm run dev --workspace=frontend",
    "dev:backend": "npm run dev --workspace=backend",
    "dev:v2-backend": "npm run dev --workspace=packages/cartrita-v2",
    "start:v2-enhanced": "npm run start --workspace=packages/cartrita-v2",
    "dev:full": "concurrently -k -n BACKEND,FRONTEND -c magenta,cyan 'npm:dev:backend' 'npm:dev:frontend'",
    "dev:mcp": "npm run dev --workspace=orchestrator",
    "dev:v2": "concurrently -k -n FASTIFY,FASTAPI,FRONTEND-V2,WORKER,RAG -c red,purple,blue,green,yellow 'npm run dev --workspace=apps/backend-v2' 'npm run dev --workspace=apps/fastapi-service' 'npm run dev --workspace=apps/frontend-v2' 'npm run dev --workspace=apps/worker' 'npm run dev --workspace=apps/rag-service'",
    "dev:rag": "npm run dev --workspace=apps/rag-service",
    "dev:worker": "npm run dev --workspace=apps/worker",
    "dev:fastapi": "npm run dev --workspace=apps/fastapi-service",
    "build": "npm run build --workspaces --if-present",
    "build:v2": "npm run build --workspaces --if-present --workspace=apps/backend-v2 --workspace=apps/frontend-v2 --workspace=apps/worker --workspace=apps/rag-service",
    "start": "npm run start:v2-enhanced",
    "start:backend": "npm run start --workspace=backend",
    "start:v1-backend": "npm run start --workspace=backend",
    "start:mcp": "npm run start --workspace=orchestrator",
    "start:v2": "concurrently -k -n BACKEND-V2,WORKER,RAG -c red,green,yellow 'npm run start --workspace=apps/backend-v2' 'npm run start --workspace=apps/worker' 'npm run start --workspace=apps/rag-service'",
    "proto:gen": "bash proto/generate.sh",
    "mcp:test": "npm test --workspaces --if-present --workspace=mcp-testing",
    "rag:evaluate": "npm run evaluate --workspace=packages/rag-evaluation",
    "rag:benchmark": "npm run benchmark --workspace=packages/rag-evaluation",
    "rag:report": "npm run report --workspace=packages/rag-evaluation",
    "prompts:lint": "npm run lint --workspace=packages/prompts",
    "prompts:test": "npm run test --workspace=packages/prompts",
    "ui:storybook": "npm run storybook --workspace=packages/ui-components",
    "ui:build": "npm run build --workspace=packages/ui-components",
    "config:validate": "npm run validate --workspace=packages/config",
    "install:all": "npm install",
    "clean": "npm run clean --workspaces --if-present",
    "lint": "eslint . --ext .js,.ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "db:migrate": "tsx db/migrate.ts",
    "db:migrate:ci": "NODE_ENV=production tsx db/migrate.ts",
    "db:migrate:force": "FORCE_MIGRATION=true tsx db/migrate.ts",
    "db:status": "psql $DATABASE_URL -c \"SELECT version, applied_at, description FROM schema_migrations ORDER BY applied_at;\"",
    "v2:migrate": "./migrate-v1-to-v2.sh",
    "v2:status": "kubectl -n data-stack get pods,svc,pvc",
    "v2:logs": "kubectl -n data-stack logs -l app=postgres --tail=100",
    "v2:connect": "kubectl -n data-stack port-forward svc/postgres 5432:5432",
    "lint:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest --config jest.config.integration.mjs",
    "test:integration": "jest --config jest.config.integration.mjs --testPathPattern=tests/integration",
    "test:migration": "jest --config jest.config.integration.mjs --testPathPattern=tests/integration/db.migration.test.ts",
    "test:vector": "jest --config jest.config.integration.mjs --testPathPattern=tests/integration/db.vector.test.ts",
    "test:integration:all": "npm run test:integration --workspaces --if-present",
    "test:e2e": "npm run test:e2e --workspaces --if-present", 
    "test:load": "npm run test:load --workspace=packages/rag-evaluation",
    "prepare": "husky",
    "secrets:scan": "detect-secrets scan > .secrets.baseline || true",
    "secrets:audit": "detect-secrets audit .secrets.baseline",
    "secrets:check": "detect-secrets scan --baseline .secrets.baseline || true",
    "docs:reorg": "bash scripts/reorganize-docs.sh",
    "docs:apply": "DRY_RUN=0 bash scripts/reorganize-docs.sh",
    "docs:build": "node scripts/build-master-readme.mjs",
    "docs:journal": "bash scripts/add_journal_entry.sh",
    "scan:keys": "node scripts/security/scan-api-keys.mjs > key-inventory.json",
    "keys:rotate": "node scripts/security/rotate-keys.mjs",
    "secrets:precommit": "node scripts/security/precommit-secret-check.mjs",
    "keys:status": "curl -s localhost:${PORT:-8001}/api/internal/keys/status || true",
    "theme:scan": "node scripts/verify-theme-consistency.js",
    "smoke:run": "bash scripts/smoke/run_all.sh",
    "faiss:setup": "python py/faiss-service/setup.py install",
    "faiss:start": "python py/faiss-service/main.py",
    "faiss:test": "python py/faiss-service/test.py",
    "monitoring:start": "npm run start --workspace=apps/monitoring",
    "monitoring:dashboard": "npm run dashboard --workspace=apps/monitoring",
    "docker:build": "docker-compose -f docker-compose.v2.yml build",
    "docker:up": "docker-compose -f docker-compose.v2.yml up -d",
    "docker:down": "docker-compose -f docker-compose.v2.yml down",
    "docker:logs": "docker-compose -f docker-compose.v2.yml logs -f"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/eslint-parser": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@tailwindcss/postcss": "^4.0.0-alpha.16",
    "@tailwindcss/typography": "^0.5.13",
    "@types/express": "^5.0.3",
    "@types/node": "^24.1.0",
    "@types/pg": "^8.11.8",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "concurrently": "^8.2.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jscodeshift": "^17.3.0",
    "lint-staged": "^16.1.5",
    "nodemon": "^3.1.10",
    "pg": "^8.12.0",
    "prettier": "^3.6.2",
    "tsx": "^4.19.1"
  },
  "description": "Personal Life OS Edition - Comprehensive AI Assistant",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Punky2280/dat-bitch-cartrita.git"
  },
  "keywords": [
    "ai",
    "assistant",
    "personal-os",
    "chatbot"
  ],
  "author": "Robert",
  "license": "AGI Commons License v4",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "bugs": {
    "url": "https://github.com/Punky2280/dat-bitch-cartrita/issues"
  },
  "homepage": "https://github.com/Punky2280/dat-bitch-cartrita#readme",
  "dependencies": {
    "@vitest/ui": "^3.2.4",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "frontend": "^0.0.0",
    "git-format-staged": "^3.1.1",
    "helmet": "^8.1.0"
  }
}
