{
  "name": "dat-bitch-cartrita-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/shared",
    "packages/robotics",
    "packages/mcp-core",
    "packages/orchestrator",
    "packages/supervisor-intelligence",
    "packages/mcp-testing",
    "packages/cartrita-v2",
    "packages/prompts",
    "packages/ui-components",
    "packages/config",
    "packages/rag-evaluation"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=packages/cartrita-v2",
    "dev:docker": "docker-compose up --build",
    "dev:logs": "docker-compose logs -f",
    "dev:down": "docker-compose down",
    "dev:clean": "docker-compose down -v",
    "dev:backend": "npm run dev --workspace=packages/cartrita-v2",
    "start": "npm run start --workspace=packages/cartrita-v2",
    "start:backend": "npm run start --workspace=packages/cartrita-v2",
    "dev:mcp": "npm run dev --workspace=orchestrator",
    "build": "npm run build --workspaces --if-present",
    "proto:gen": "bash proto/generate.sh",
    "mcp:test": "npm test --workspaces --if-present --workspace=mcp-testing",
    "rag:evaluate": "npm run evaluate --workspace=packages/rag-evaluation",
    "rag:benchmark": "npm run benchmark --workspace=packages/rag-evaluation",
    "rag:report": "npm run report --workspace=packages/rag-evaluation",
    "prompts:lint": "npm run lint --workspace=packages/prompts",
    "prompts:test": "npm run test --workspace=packages/prompts",
    "ui:storybook": "npm run storybook --workspace=packages/ui-components",
    "ui:build": "npm run build --workspace=packages/ui-components",
    "config:validate": "npm run validate --workspace=packages/config",
    "install:all": "npm install",
    "clean": "npm run clean --workspaces --if-present",
    "lint": "eslint . --ext .js,.ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "db:migrate": "tsx db/migrate.ts",
    "db:migrate:ci": "NODE_ENV=production tsx db/migrate.ts",
    "db:migrate:force": "FORCE_MIGRATION=true tsx db/migrate.ts",
    "db:status": "psql $DATABASE_URL -c \"SELECT version, applied_at, description FROM schema_migrations ORDER BY applied_at;\"",
    "lint:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest --config jest.config.integration.mjs",
    "test:integration": "jest --config jest.config.integration.mjs --testPathPattern=tests/integration",
    "test:migration": "jest --config jest.config.integration.mjs --testPathPattern=tests/integration/db.migration.test.ts",
    "test:vector": "jest --config jest.config.integration.mjs --testPathPattern=tests/integration/db.vector.test.ts",
    "test:integration:all": "npm run test:integration --workspaces --if-present",
    "test:e2e": "npm run test:e2e --workspaces --if-present",
    "test:load": "npm run test:load --workspace=packages/rag-evaluation",
    "prepare": "husky",
    "secrets:scan": "detect-secrets scan > .secrets.baseline || true",
    "secrets:audit": "detect-secrets audit .secrets.baseline",
    "secrets:check": "detect-secrets scan --baseline .secrets.baseline || true",
    "faiss:setup": "python py/faiss-service/setup.py install",
    "faiss:start": "python py/faiss-service/main.py",
    "faiss:test": "python py/faiss-service/test.py",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/eslint-parser": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@tailwindcss/postcss": "^4.0.0-alpha.16",
    "@tailwindcss/typography": "^0.5.13",
    "@types/express": "^5.0.3",
    "@types/node": "^24.1.0",
    "@types/pg": "^8.11.8",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "concurrently": "^8.2.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jscodeshift": "^17.3.0",
    "lint-staged": "^16.1.5",
    "nodemon": "^3.1.10",
    "pg": "^8.12.0",
    "prettier": "^3.6.2",
    "tsx": "^4.19.1"
  },
  "description": "Personal Life OS Edition - Comprehensive AI Assistant",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Punky2280/dat-bitch-cartrita.git"
  },
  "keywords": [
    "ai",
    "assistant",
    "personal-os",
    "chatbot"
  ],
  "author": "Robert",
  "license": "AGI Commons License v4",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "bugs": {
    "url": "https://github.com/Punky2280/dat-bitch-cartrita/issues"
  },
  "homepage": "https://github.com/Punky2280/dat-bitch-cartrita#readme",
  "dependencies": {
    "@vitest/ui": "^3.2.4",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "git-format-staged": "^3.1.1",
    "helmet": "^8.1.0"
  }
}
