# Backend service environment example (sanitized)
NODE_ENV=development
SERVICE_NAME=backend
PORT=4000

# Provider API keys (placeholders)
OPENAI_API_KEY=sk-REDACTED
ANTHROPIC_API_KEY=anthropic-REDACTED
DEEPGRAM_API_KEY=dg-REDACTED
HUGGINGFACE_API_KEY=hf_REDACTED

# Hugging Face Router (optional tuning)
# Enable lightweight probe pings & adjust candidates
HF_ROUTER_ENABLE_PROBES=true
HF_ROUTER_PROBE_TIMEOUT_MS=3500
HF_ROUTER_MAX_CANDIDATES=6

# Encryption / security
ENCRYPTION_MASTER_KEY=change-me
JWT_SECRET=change-me-jwt

# Database
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=cartrita
POSTGRES_USER=cartrita
POSTGRES_PASSWORD=change-me

# Embeddings / vector
PGVECTOR_DIM=1536

# Feature flags
ENABLE_HF_BRIDGE=true
ENABLE_VAULT=true
ENABLE_UNIX_SOCKET=true
