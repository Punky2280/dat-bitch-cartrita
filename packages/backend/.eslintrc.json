{
  "env": {
    "node": true,
    "es2022": true
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "requireConfigFile": false,
    "babelOptions": {
      "presets": ["@babel/preset-env"]
    }
  },
  "extends": ["eslint:recommended"],
  "rules": {
    "no-console": "off",
    "no-unused-vars": "off",
    "no-undef": "off",
    "prefer-const": "off",
    "no-var": "off",
    "no-useless-escape": "off",
    "no-redeclare": "off",
    "no-unreachable": "off",
    "no-constant-condition": "off",
    "no-empty": "off",
    "no-irregular-whitespace": "off",
    "no-prototype-builtins": "off",
    "no-fallthrough": "off",
    "no-case-declarations": "off",
    "no-self-assign": "off",
    "no-global-assign": "off",
    "no-unsafe-negation": "off"
  },
  "globals": {
    "process": "readonly",
    "Buffer": "readonly",
    "__dirname": "readonly",
    "__filename": "readonly",
    "global": "readonly",
    "console": "readonly",
    "require": "readonly",
    "module": "readonly",
    "exports": "readonly"
  }
}
